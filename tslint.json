{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": ["node_modules/**", "bin/**", "**/bin/**"]
    },
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "no-unused-variable": [true, {"ignore-pattern": "^_"}]
    },
    "rulesDirectory": []
}
